{  "parser": "@typescript-eslint/parser",  "parserOptions": {    "ecmaVersion": 12,    "ecmaFeatures": {      "jsx": true    },    "sourceType": "module"  },  "plugins": ["@typescript-eslint", "react", "prettier"],  "extends": [    "eslint:recommended",    "plugin:@typescript-eslint/recommended"  ],  "rules": {    "@typescript-eslint/no-unused-vars": "error",    "@typescript-eslint/consistent-type-definitions": ["error", "type"],    "@typescript-eslint/no-explicit-any": "off",    "@typescript-eslint/ban-ts-comment": "off"  },  "env": {    "browser": true,    "es2022": true  }}